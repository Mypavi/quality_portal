{
  "_version": "1.76.0",
  "sap.app": {
    "id": "quality",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "0d7f2416-8e9e-4ae2-855e-59c4786d8017"
    },
    "dataSources": {
      "loginService": {
        "uri": "/sap/opu/odata/sap/ZQM_LOG_PR_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "useBatch": true
        }
      },
      "inspectionService": {
        "uri": "/sap/opu/odata/sap/ZQM_INSPECT_PR_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "useBatch": true
        }
      },
      "resultService": {
        "uri": "/sap/opu/odata/sap/ZQM_RESULT_PR_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "useBatch": true,
          "defaultBindingMode": "TwoWay"
        }
      },
      "usageService": {
        "uri": "/sap/opu/odata/sap/ZQM_US_PR_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "useBatch": true
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "quality.i18n.i18n"
        }
      },
      "auth": {
        "dataSource": "loginService",
        "preload": true
      },
      "inspection": {
        "dataSource": "inspectionService",
        "preload": true
      },
      "result": {
        "dataSource": "resultService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline"
        }
      },
      "usage": {
        "dataSource": "usageService",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "quality.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": "",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "RouteDashboard",
          "pattern": "dashboard",
          "target": [
            "TargetDashboard"
          ]
        },
        {
          "name": "RouteResultRecordingList",
          "pattern": "result",
          "target": [
            "TargetResultRecording"
          ]
        },
        {
          "name": "RouteResultRecording",
          "pattern": "result/{inspectionLot}",
          "target": [
            "TargetResultRecording"
          ]
        },
        {
          "name": "RouteUsageDecisionList",
          "pattern": "usage",
          "target": [
            "TargetUsageDecision"
          ]
        },
        {
          "name": "RouteUsageDecision",
          "pattern": "usage/{inspectionLot}",
          "target": [
            "TargetUsageDecision"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "viewName": "Login",
          "viewId": "login"
        },
        "TargetDashboard": {
          "viewName": "Dashboard",
          "viewId": "dashboard"
        },
        "TargetResultRecording": {
          "viewName": "ResultRecording",
          "viewId": "resultRecording"
        },
        "TargetUsageDecision": {
          "viewName": "UsageDecision",
          "viewId": "usageDecision"
        }
      }
    },
    "rootView": {
      "viewName": "quality.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}